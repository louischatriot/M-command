{
  "name": "M command",
  "version": "1.0",
  "manifest_version" : 2,
  "description": "Vim style m command for Chrome",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-1.7.2.min.js", "mcommand.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": []
}
